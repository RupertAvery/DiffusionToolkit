<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:DiffusionToolkit.AvaloniaApp.ViewModels"
        xmlns:thumbnail="clr-namespace:DiffusionToolkit.AvaloniaApp.Controls.Thumbnail"
        xmlns:search="clr-namespace:DiffusionToolkit.AvaloniaApp.Pages.Search"
        mc:Ignorable="d" d:DesignWidth="1200" d:DesignHeight="800"
        x:Class="DiffusionToolkit.AvaloniaApp.MainWindow"
        Title="DiffusionToolkit.AvaloniaApp">

  <Design.DataContext>
    <viewModels:MainWindowViewModel/>
  </Design.DataContext>
  <!--
          ExtendClientAreaChromeHints="NoChrome" 
        ExtendClientAreaToDecorationsHint="True"
  -->
  <Grid>
      <ContentControl Content="{Binding CurrentPage}"></ContentControl>
  </Grid>
</Window>
